{
  "name": "hyparquet",
  "version": "1.8.1",
  "description": "parquet file parser for javascript",
  "author": "Hyperparam",
  "homepage": "https://hyperparam.app",
  "keywords": [
    "hyparquet",
    "parquet",
    "parquetjs",
    "parser",
    "snappy",
    "thrift"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyparam/hyparquet.git"
  },
  "main": "src/hyparquet.js",
  "files": [
    "types",
    "src"
  ],
  "type": "module",
