    "@vitest/coverage-v8": "3.0.1",
    "eslint": "9.18.0",
    "eslint-plugin-jsdoc": "50.6.1",
    "hyparquet-compressors": "1.0.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.20.0",
    "vitest": "3.0.1"
  }
